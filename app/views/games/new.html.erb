<div id="top">
  <h1>New</h1>
  <%= link_to "go to score", score_path, id:"link" %>
</div>

<p id="letters"><%= @letters %></p>
<div class="cases">
  <span id="letters"><%= @l1 %></span>
  <span id="letters"><%= @l2 %></span>
  <span id="letters"><%= @l3 %></span>
  <span id="letters"><%= @l4 %></span>
  <span id="letters"><%= @l5 %></span>
  <span id="letters"><%= @l6 %></span>
  <span id="letters"><%= @l7 %></span>
  <span id="letters"><%= @l8 %></span>
  <span id="letters"><%= @l9 %></span>
  <span id="letters"><%= @l10 %></span>
</div>
<div class="form">
  <form action="/score" method="post" data-turbo="false">
  <input type="text" name="score">
  <input type="submit" value="Score!">
  <!-- ... -->
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= hidden_field_tag :letters, @letters %>
</form>
</div>
